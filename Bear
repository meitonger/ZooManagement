import java.util.Scanner;


public class Bear  implements infoReaderAnimals{

	public Bear() {

	}
	


	
	public String Info() {

        String Info = 
        	
				"Name: BEAR"+" Color: Brown"+" Age: 2";
		return Info;
	}



	@Override
	public void information(Animals animals) {
        System.out.println("Animals information " + animals.getName() +animals.getColor()+animals.getAge());
		
	}
	
	
	  
	
    
	




	
	

	
	
	




}
