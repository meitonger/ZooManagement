
public class Elephant implements infoReaderAnimals {
	
	public Elephant() {

	}
	

    
	public String Info() {

        String Info = 
				"Name: Elephant"+" Color: Grey"+" Age: 1";
		return Info;
	}


	@Override
	public void information(Animals animals) {
        System.out.println("Animals information " + animals.getName() +animals.getColor()+animals.getAge());
		
	}
}
